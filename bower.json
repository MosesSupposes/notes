{
  "name": "cryptag-notes",
  "description": "Secure note-taking and collaborative wiki page editing app",
  "main": "main.js",
  "authors": [
    "jimmcgaw <jpmcgaw@gmail.com>",
    "Steve Phillips <steve@tryingtobeawesome.com>"
  ],
  "license": "AGPLv3",
  "keywords": [
    "CrypTag",
    "CrypTag Notes"
  ],
  "homepage": "https://github.com/cryptag/notes",
  "ignore": [
    "**/.*",
    "node_modules",
