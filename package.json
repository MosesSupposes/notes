{
  "name": "cryptag-notes",
  "version": "0.8.0",
  "description": "Secure note-taking and collaborative wiki page editing app",
  "main": "main.js",
  "bugs": {
    "url": "https://github.com/cryptag/notes/issues"
  },
  "scripts": {
    "start": "node_modules/.bin/electron main.js",
    "build": "electron-packager ./ cryptag-notes --platform=darwin --arch=x64 --overwrite",
    "build-linux": "electron-packager ./ cryptag-notes --platform=linux --arch=x64 --overwrite",
    "build-windows": "electron-packager ./ cryptag-notes --platform=win32 --arch=ia32 --overwrite",
    "test": "node_modules/.bin/mocha test/.setup.js test/**/*.test.js"
  },
  "keywords": [
    "CrypTag",
    "CrypTag Notes"
  ],
  "author": "",
  "license": "AGPLv3",
  "devDependencies": {
    "atob": "^2.0.3",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "btoa": "^1.1.2",
    "chai": "^3.5.0",
    "electron-chromedriver": "^1.2.1",
    "electron-packager": "^7.2.0",
    "electron-prebuilt": "^1.2.6",
    "enzyme": "^2.3.0",
    "gulp": "^3.9.1",
    "gulp-inject": "^4.1.0",
    "gulp-sass": "^2.3.2",
    "react-addons-test-utils": "^15.1.0",
    "shelljs": "^0.7.5",
    "spectron": "^3.2.3",
    "webdriverio": "^4.1.1",
    "wiredep": "^4.0.0"
  },
  "dependencies": {
    "fs-extra": "^1.0.0",
    "jsdom": "^9.8.3",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "node-notifier": "^4.6.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.1.0",
    "react-markdown": "^2.4.2",
    "react-widgets": "^3.4.5",
    "utf8": "^2.1.1"
  }
}
